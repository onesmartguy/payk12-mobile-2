{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/common/*": [
        "./features/common/*"
      ],
      "@/redemption/*": [
        "./features/redemption/*"
      ],
      "@/ticketing/*": [
        "./features/ticketing/*"
      ],
      "@/auth/*": [
        "./features/auth/*"
      ],
      "@/*": [
        "./*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
, "app.config.ts.bak"  ],
  "exclude": [
    "!old_src/**/*"
  ]
}
